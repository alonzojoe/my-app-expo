{
  "expo": {
    "scheme": "myapp",
    "name": "Lingad Care",
    "slug": "lingad-care",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-finalv.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.alonzojoe.lingadcare",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "We need access to your camera to take photos."
      }
    },
    "android": {
      "icon": "./assets/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./assets/logo-finalv.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "mdpi": "./assets/logo-finalv.png",
        "hdpi": "./assets/logo-finalv.png",
        "xhdpi": "./assets/logo-finalv.png",
        "xxhdpi": "./assets/logo-finalv.png",
        "xxxhdpi": "./assets/logo-finalv.png"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "CAMERA"
      ],
      "package": "com.alonzojoe.lingadcare",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3cbeead1-6191-4089-b119-6ba1c07a9044"
      }
    },
    "owner": "alonzojoe07",
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
