{
  "expo": {
    "scheme": "myapp",
    "name": "Lingad Care",
    "slug": "lingad-care",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-final.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-final.png",
      "resizeMode": "contain",
      "backgroundColor": "#001C63"
    },
    "expo": {
      "updates": {
        "enabled": true,
        "fallbackToCacheTimeout": 0
      }
    },
    "ios": {
      "bundleIdentifier": "com.myapp",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "We need access to your camera to take photos."
      }
    },
    "android": {
      "icon": "./assets/logo-final.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-final.png",
        "backgroundColor": "#001C63"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "INTERNET",
        "CAMERA"
      ],
      "package": "com.alonzojoe.lingadcare",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c99643bb-4b0a-4397-863f-eb2d7e98a738"
      }
    },
    "owner": "alonzojoes-organization"
  }
}
